<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.csc325firebase.PrimaryController">
   <center>
      <Pane prefHeight="420.0" prefWidth="496.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="nameTF" layoutX="50.0" layoutY="30.0" prefHeight="26.0" prefWidth="409.0" promptText="name" />
            <TextField fx:id="ageTF" layoutX="50.0" layoutY="70.0" prefHeight="26.0" prefWidth="409.0" promptText="age" />
            <Button layoutX="90.0" layoutY="150.0" mnemonicParsing="false" onAction="#readButtonHandler" text="Read" />
            <Button fx:id="writeButton" layoutX="180.0" layoutY="150.0" mnemonicParsing="false" onAction="#writeButtonHandler" text="Write" />
            <Button layoutX="270.0" layoutY="150.0" mnemonicParsing="false" onAction="#registerButtonHandler" text="Register" />
            <Button layoutX="360.0" layoutY="150.0" mnemonicParsing="false" onAction="#buttonHandler" text="Button" />
            <TextArea fx:id="textFieldTF" layoutX="13.0" layoutY="191.0" prefHeight="210.0" prefWidth="471.0" />
         </children>
      </Pane>
   </center>
